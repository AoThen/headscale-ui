{
	skip_install_trust
	auto_https disable_redirects
	http_port {$HTTP_PORT}
	https_port {$HTTPS_PORT}
}

:{$HTTP_PORT} {
	redir / /webXui
	uri strip_prefix /webXui
	file_server {
		root /webXui
	}
}

:{$HTTPS_PORT} {
	redir / /webXui
	uri strip_prefix /webXui
	tls {$TLS_CERT_PATH} {$TLS_KEY_PATH}
	file_server {
		root /webXui
	}
}
